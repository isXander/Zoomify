{
  "schemaVersion": 1,
  "id": "zoomify",
  "version": "${version}",
  "name": "Zoomify",
  "description": "A zoom mod with infinite customizability.",
  "authors": [
    "isXander"
  ],
  "contact": {
    "homepage": "https://isxander.dev",
    "issues": "https://github.com/isXander/Zoomify/issues",
    "sources": "https://github.com/isXander/Zoomify"
  },
  "icon": "assets/zoomify/zoomify.png",
  "license": "LGPLv3",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.isxander.zoomify.Zoomify"
      }
    ],
    "preLaunch": [
      "com.llamalad7.mixinextras.MixinExtrasBootstrap::init"
    ],
    "modmenu": [
      {
        "adapter": "kotlin",
        "value": "dev.isxander.zoomify.integrations.ModMenuIntegration"
      }
    ]
  },
  "mixins": [
    "zoomify.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabricloader": ">=0.13.0",
    "fabric-language-kotlin": ">=1.7.4+kotlin.1.6.21",
    "minecraft": "1.19.x",
    "java": ">=17",
    "cloth-config": "7.x.x"
  },
  "suggests": {
    "modmenu": ">=3.0.0"
  }
}
